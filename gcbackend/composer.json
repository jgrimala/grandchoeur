    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "firebase/php-jwt": "^6.10",
        "graham-campbell/result-type": "^1.1",
        "phpoption/phpoption": "^1.9",
        "vlucas/phpdotenv": "^5.6",
        "symfony/polyfill-ctype": "^1.29",
        "symfony/polyfill-mbstring": "^1.29",
        "symfony/polyfill-php80": "^1.29"
    },
    "autoload": {
        "psr-4": {
